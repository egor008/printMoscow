<!DOCTYPE html>
<html>
<head>
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<meta charset="UTF-8">
	<link rel="stylesheet" type="text/css" href="~/css/normalize.css" />
	<link rel="stylesheet" type="text/css" href="~/css/style.css">
	<link rel="stylesheet" href="~/css/owl.carousel.min.css"/>
	<link rel="stylesheet" href="~/css/owl.theme.default.min.css" />
	<link rel="stylesheet" href="~/css/lightbox.min.css">
	<title>PRINT.MOSCOW</title>
</head>
<body>
	<header>
		<div class="header">
			<div class="header__logo">
				<a asp-action="Index"
				   asp-controller="Home">
					<img src="~/img/logo.png">
				</a>

			</div>
			<div></div>
			<div></div>
			<div class="header__phonenumber">
				<!-- <div>
					<a href="tel:+74957998006">+7 (495) 799-80-06</a>
				</div> -->
				<figure>
					<a href="tel:+74957998006">
						<img src="~/img/phone.png" alt="1">
						<figcaption>
							<span>+7 (495) 799-80-06</span>
						</figcaption>
					</a>
				</figure>
				<figure>
					<a href="mailto:+74957998006">
						<img src="~/img/mail.png" alt="1">
						<figcaption>
							<span>info@print.moscow</span>
						</figcaption>
					</a>
				</figure>
				<figure>
					<a href="https://www.instagram.com/print_moscow?hl=ru">
						<img src="~/img/inst.png" alt="1">
						<figcaption>
							<span>@@print_moscow</span>
						</figcaption>
					</a>
				</figure>
			</div>
			<a href="#" id="menu" class="icon"><i class="iconss fas fa-align-justify"></i></a>
		</div>

		<nav class="header__desktop">

			<div class="topnav">
				<ul class="menu">
					<li>
						<a asp-action="Index"
						   asp-controller="Home">Главная</a>
					</li>
					@*<li>
						<a href="photo.html" class="scrollto">Фото работ</a>
					</li>*@
					<li>
						<a asp-action="Index"
						   asp-controller="Home"
						   asp-fragment="products" class="scrollto">Наша продукция</a>
					</li>
					<!-- <li>
						<a href="photo.html" class="scrollto">Отзывы</a>
					</li> -->
					<li>
						<a asp-action="Index"
						   asp-controller="Home" asp-fragment="contacts" class="scrollto">Контакты</a>
					</li>
					<li>
						@await Component.InvokeAsync("CartSummary")
					</li>
				</ul>

			</div>


		</nav>
		<nav class="header__phone">
			<div class="topnav" id="myTopnav">
				<a asp-action="Index"
				   asp-controller="Home" class="scrollto">Главная</a>
				<div style="display: flex;justify-content: space-between;">
					<a asp-action="Index"
					   asp-controller="Home" class="scrollto">Наша продукция</a>
					<a href="#" id="menus" class="icon scrollto"><i class="icons fas fa-angle-down"></i></a>
				</div>
				<div class="topnas" id="myTopnas">
				</div>
				@*<a href="photo.html" class="scrollto">Фото работ</a>*@
				<a class="lastUrl scrollto" asp-action="Index"
				   asp-controller="Home">Контакты</a>

			</div>
		</nav>
	</header>

	@*<div class="navbar navbar-inverse" role="navigation">
			<a class="navbar-brand" href="#">PrintMoscow</a>
			<div class="pull-right">
				@await Component.InvokeAsync("CartSummary")
			</div>
		</div>*@

